{
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "css.lint.unknownAtRules": "ignore",
  "css.lint.validProperties": [],
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "tailwindCSS.includeLanguages": {
    "css": "css",
    "html": "html",
    "javascript": "javascript",
    "javascriptreact": "javascript"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
} 