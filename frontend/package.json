{
  "name": "acting-scripts-frontend",
  "version": "0.1.0",
  "description": "연기 대본 라이브러리 프론트엔드",
  "private": true,
  "dependencies": {
    "@supabase/supabase-js": "^2.53.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.6.7",
    "express": "^4.18.0",
    "framer-motion": "^11.0.3",
    "lucide-react": "^0.323.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.22.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development react-scripts start",
    "start": "node server.js",
    "start:dev": "cross-env NODE_ENV=development react-scripts start",
    "build": "cross-env NODE_ENV=production react-scripts build",
    "build:dev": "cross-env NODE_ENV=development react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "serve": "npx serve -s build",
    "build:serve": "npm run build && npm start",
    "debug:env": "node -e \"console.log('Environment Variables:'); Object.keys(process.env).filter(key => key.startsWith('REACT_APP')).forEach(key => console.log(key + '=' + process.env[key]))\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5"
  }
}
